{
    "dataRoles": [
        {
            "displayName": "Category",
            "name": "category",
            "kind": "Grouping"
        },
        {
            "displayName": "Measure",
            "name": "measure",
            "kind": "Measure"
        },
        {
            "displayName": "Min value measure",
            "name": "minMeasureValue",
            "kind": "Measure"
        },
        {
            "displayName": "Max value measure",
            "name": "maxMeasureValue",
            "kind": "Measure"
        },
        {
            "displayName": "Tooltip",
            "name": "tooltip",
            "kind": "Measure"
        }
    ],
    "objects": {
        "barShape": {
            "displayName": "Bar shape",
            "properties": {
                "color": {
                    "displayName": "Default color",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "showAll": {
                    "displayName": "Show all",
                    "type": {
                        "bool": true
                    }
                }
            }
        }
    },
    "dataViewMappings": [
        {
            "conditions": [
                {
                    "category": {
                        "max": 1
                    },
                    "measure": {
                        "max": 1
                    },
                    "minMeasureValue": {
                        "max": 1
                    },
                    "maxMeasureValue": {
                        "max": 1
                    }
                }
            ],
            "categorical": {
                "categories": {
                    "for": {
                        "in": "category"
                    }
                },
                "values": {
                    "select": [
                        {
                            "bind": {
                                "to": "measure"
                            }
                        },
                        {
                            "bind": {
                                "to": "minMeasureValue"
                            }
                        },
                        {
                            "bind": {
                                "to": "maxMeasureValue"
                            }
                        },
                        {
                            "bind": {
                                "to": "tooltip"
                            }
                        }
                    ]
                }
            }
        }
    ],
    "tooltips": {
        "supportedTypes": {
            "default": true,
            "canvas": true
        },
        "roles": [
            "tooltips"
        ],
        "supportEnhancedTooltips": true
    },
    "supportsLandingPage": true,
    "supportsEmptyDataView": true,
    "suppressDefaultTitle": true
}